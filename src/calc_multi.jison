/* Simple calculator language with variable substitution using jison *//* inspired by https://gist.github.com/richard-to/8109536 */%lex%{    var parser = yy.parser;%}%%\n                    return 'EOL'\s+                   /* skip whitespace */"$"[A-Z]+[A-Z0-9]*\b  return 'VARIABLE'[0-9]+("."[0-9]+)?\b  return 'NUMBER'"="                   return '='"-"                   return '-'"+"                   return '+'"/"                   return '/'"*"                   return '*'"^"                   return '^'"("                   return '('")"                   return ')'"PI"                  return 'PI'"E"                   return 'E'<<EOF>>               return 'EOF'/lex%start calc%right '='%left '+' '-'%left '*' '/'%right '^'%left UMINUS%%calc    : lists EOF        {            var results = yy.parser.getResults();            yy.parser.clearResults();            typeof console !== 'undefined' ? console.log(results) : print(results);            return results;        }    ;lists    : expr EOL lists        { yy.parser.addResult($1); }    | expr        { yy.parser.addResult($1); }    ;expr    : primary        {$$ = $1;}    | VARIABLE '=' expr        {            yy.parser.setVar($1, $3);            $$ = $3;        }    | expr '+' expr        {$$ = $1 + $3;}    | expr '-' expr        {$$ = $1 - $3;}    | expr '*' expr        {$$ = $1 * $3;}    | expr '/' expr        {$$ = $1 / $3;}    | expr '^' expr        {$$ = Math.pow($1, $3);}    ;primary    : NUMBER        {$$ = Number(yytext);}    | VARIABLE        {$$ =  yy.parser.getVar($1);}    | '(' expr ')'        {$$ =  $2;}    | '-' expr %prec UMINUS        {$$ = -$2;}    | E        {$$ = Math.E;}    | PI        {$$ = Math.PI;}    ;%%parser.addResult = function(value) {    if (!this._results) {        this._results = [];    }    this._results.unshift(value);}parser.getResults = function() {    return this._results;}parser.clearResults = function() {    this._results = [];}parser.getVar = function(key) {    if (!this._varTable) {        return undefined;    } else {        return this._varTable[key];    }}parser.setVar = function(key, val) {    if (!this._varTable) {        this._varTable = {};    }    this._varTable[key] = val;}